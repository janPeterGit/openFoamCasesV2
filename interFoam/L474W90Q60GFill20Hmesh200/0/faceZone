FoamFile
{
    version     2.0;
    format      ascii;
    class       surfaceScalarField;
    location    "0";
    object      faceZone;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];


internalField   uniform -1;

boundaryField
{
    bottom
    {
        type            calculated;
        value           uniform -1;
    }
    cylinder
    {
        type            calculated;
        value           uniform -1;
    }
    frontAndBack
    {
        type            calculated;
        value           uniform -1;
    }
    inlet
    {
        type            calculated;
        value           uniform -1;
    }
    outletAir
    {
        type            calculated;
        value           uniform -1;
    }
    outletWater
    {
        type            calculated;
        value           uniform -1;
    }
    top
    {
        type            calculated;
        value           uniform -1;
    }
}


