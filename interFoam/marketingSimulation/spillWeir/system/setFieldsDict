FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volVectorFieldValue U (0 0 0)
    volScalarFieldValue alpha.water 0
);

regions
(   
    boxToCell
    {
        box (-1 -0.2045 0) (2 0.2045 0.3);
        fieldValues
        (
            volVectorFieldValue U (0 0 0)
            volScalarFieldValue alpha.water 1
        );
    }
    /*
