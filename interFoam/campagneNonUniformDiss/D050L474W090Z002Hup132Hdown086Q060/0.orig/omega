FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "startBC/initialConditions"

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform $omegaInlet;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }
    /*
    outlet
    {
        type    zeroGradient;
    }
    */
    outletWater
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

	outletAir
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    /*
    outletAir
    {
		type            zeroGradient; 
    }
	*/
	top
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField; // später hinzugefügt, testen?
    }    
	
	"(bottom|frontAndBack)"
    {
        type            omegaWallFunction;
        value           $omegaHighRe;
    }

	obstacle
    {
        type            omegaWallFunction;
        value           $omegaLowRe;
    }

    
}

// ************************************************************************* //
